        "# Encontrar la region que mÃ¡s vendiÃ³\n",
        "top_region <- df %>%\n",
        "  group_by(Category) %>%\n",
        "  summarise(Total_Ventas = sum(Sales)) %>%\n",
        "  arrange(desc(Total_Ventas)) %>%\n",
        "  slice(1)\n",
