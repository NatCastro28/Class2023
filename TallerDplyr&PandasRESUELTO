        "\n",
        "ggplot(data= mean_sales, aes(x= Region, y= mean_sales, color =Region, shape = Region)) +\n",
        "  geom_point() +\n",
        "  labs(title = \"Relación entre ventas y profit por región\", x = \"Region\", y = \"Media de Ventas\") \n"
      ],
      "metadata": {
